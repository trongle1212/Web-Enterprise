@{
    Layout = "~/Areas/Admin/Views/Shared/_LayoutPage.cshtml";
}
@model Core.DTO.Account.AccountDTO
<div class="account-management">
    @Html.Partial("_breadcrumb", new Dictionary<string, string>
        { { "Home", "Homepage" }, { "AccountManagement", "Account management" }, { "C", "Add new" } }, new ViewDataDictionary(ViewData) { { "mHref", Url.Action("Index", "AccountManagement") }, { "mShowButton", "false" } })

        @using (Html.BeginForm("New", "AccountManagement", FormMethod.Post, new { @id = "form-create", @class = "form-horizontal needs-validation", @enctype = "multipart/form-data" }))
        {
            @Html.Partial("_Form", Model)
        }
</div>

